COMPOSE_PROJECT_NAME=hackathon2021

# Configure host names, which will be used to configure Traefik proxy routing.
CM_HOST=cm.hackathon2021.localhost
CD_HOST=cd.hackathon2021.localhost
ID_HOST=id.hackathon2021.localhost
RENDERING_HOST=www.hackathon2021.localhost
HOST_DOMAIN=hackathon2021.localhost

# Sitecore Docker registry and platform version.
# The ltsc2019-based images are used by default here.
SITECORE_DOCKER_REGISTRY=scr.sitecore.com/sxp/
SITECORE_VERSION=10.1-ltsc2019
SPE_VERSION=6.2-1809
SXA_VERSION=10.1-1809

# The sitecore\admin and SQL 'sa' account passwords for this environment are configurable here.
SITECORE_ADMIN_PASSWORD=b
